struct pair getMinMax(long long int arr[], long long int n) {
    struct pair result;
    result.min = arr[0];
    result.max=arr[0];
    for(int i=0;i<n;i++){
        if(result.max<arr[i])
            result.max=arr[i];
        if(result.min>arr[i])
            result.min=arr[i];
    }
    return result;
    
}
